package qa_cinema.Tickets;

public class ChildTicket extends Tickets {

	private int price;
	private String Type;
	private int total;
	
	public ChildTicket(int numberOfTickets, String TicketType) {
		super(numberOfTickets, TicketType);
		this.price = 4;
		this.Type = "Child";
	}
	
	public int getCTPrice() {
		return price * total;
	}
	
	public String getCTType() {
		return Type;
	}
}
